{
  "name": "@sitearcade/parse-node",
  "version": "0.1.2",
  "description": "Content parsers and transformers.",
  "license": "MIT",
  "homepage": "https://www.sitearcade.com",
  "repository": "github:sitearcade/parse-node",
  "bugs": "https://github.com/sitearcade/parse-node/issues",
  "author": {
    "name": "Mike Stop Continues",
    "email": "mike@sitearcade.com",
    "url": "https://www.sitearcade.com"
  },
  "sideEffects": false,
  "source": "src/index.js",
  "main": "./dist/index.js",
  "jsnext:main": "./dist/index.modern.js",
  "module": "./dist/index.module.js",
  "syntax": {
    "esmodules": "./dist/index.modern.js"
  },
  "exports": {
    ".": {
      "browser": "./dist/index.module.js",
      "import": "./dist/index.module.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "microbundle watch --no-compress -f cjs,esm,modern --jsx React.createElement --jsxImportSource react",
    "build": "microbundle --no-compress -f cjs,esm,modern --jsx React.createElement --jsxImportSource react",
    "clean": "rm -rf dist",
    "test": "NODE_ENV=test jest --passWithNoTests --coverage",
    "test:ci": "NODE_ENV=test jest --passWithNoTests --ci",
    "test:watch": "NODE_ENV=test jest --passWithNoTests --coverage --watch",
    "preversion": "npm run build && npm test",
    "postversion": "npm publish --access public",
    "prepublishOnly": "npm run clean && npm run build",
    "postpublish": "git push origin --all; git push origin --tags"
  },
  "dependencies": {
    "@fec/remark-a11y-emoji": "^3.0.0",
    "gray-matter": "^4.0.2",
    "ramda": "^0.27.1",
    "rehype-stringify": "^8.0.0",
    "remark-autolink-headings": "^6.0.1",
    "remark-breaks": "^2.0.1",
    "remark-emoji": "^2.1.0",
    "remark-external-links": "^8.0.0",
    "remark-images": "^2.0.0",
    "remark-oembed": "^1.2.1",
    "remark-parse": "^9.0.0",
    "remark-rehype": "^8.0.0",
    "remark-slug": "^6.0.0",
    "remark-squeeze-paragraphs": "^4.0.0",
    "remark-textr": "^4.0.0",
    "remark-underline": "^1.0.5",
    "remark-unwrap-images": "^2.0.0",
    "striptags": "^3.1.1",
    "typographic-base": "^1.0.4",
    "unified": "^9.2.0",
    "unist-util-visit": "^2.0.3"
  },
  "devDependencies": {
    "@sitearcade/babel-preset": "0.5.0",
    "@sitearcade/browserslist-config": "0.3.0",
    "@sitearcade/eslint-config": "0.3.0",
    "@sitearcade/jest-preset": "2.1.0",
    "microbundle": "0.13.0"
  },
  "engines": {
    "node": ">=10"
  },
  "babel": {
    "presets": [
      "module:@sitearcade/babel-preset"
    ]
  },
  "browserslist": [
    "extends @sitearcade/browserslist-config"
  ],
  "eslintConfig": {
    "extends": [
      "@sitearcade/eslint-config"
    ]
  },
  "jest": {
    "preset": "@sitearcade/jest-preset"
  }
}
